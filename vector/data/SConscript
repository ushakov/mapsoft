Import ('env')

env.Install(env.datadir, Split("""
  pics/ mmb.cnf hr.cnf default.cnf
"""))

env.Install(env.datadir, Split("pics/*.fig pics/*.png"))

env.MProg('zn_figlib',
	  srcs = ['zn_figlib.cpp'],
          deps = ['vmap'])

env.MProg('zn_legend',
	  srcs = ['zn_legend.cpp'],
          deps = ['vmap'])

env.MProg('zn_text',
	  srcs = ['zn_text.cpp'],
          deps = ['vmap'])
