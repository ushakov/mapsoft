
mapsoft_libs = Split ("vmap geo_io geo loaders jeeps fig mp options utils 2d")
other_libs   = Split ("curl usb tiff jpeg png yaml proj")

Import ('env_cairo')
e = env_cairo.Clone()
e.Prepend(LIBS=[mapsoft_libs,other_libs])

e.Program('mapsoft_vmap.cpp')
e.Program('fig2pnm.cpp')
e.Program('get_print.cpp')
e.Program('get_print_mmb.cpp')
e.Program('make_map_gk.cpp')
e.Program('make_nom.cpp')

